{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "next/core-web-vitals",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "prefer-const": "error",
    "quotes": [
      "error",
      "double"
    ]
  }
}